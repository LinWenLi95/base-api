<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwl.base.project.mapper.SysUserMapper">

    <select id="getPage" resultType="com.lwl.base.project.vo.GetUserPageVO">
        select
        id,
        nick_name,
        username,
        telephone,
        email,
        state,
        description,
        creator_id,
        create_time,
        updater_id,
        update_time
        from sys_user
        ${ew.customSqlSegment}
    </select>
</mapper>
